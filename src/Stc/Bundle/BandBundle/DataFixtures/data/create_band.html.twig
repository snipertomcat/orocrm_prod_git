@entityName = Stc\Bundle\BandBundle\Entity\Band
@subject = A new band was created
@isSystem = 1

<h1>A new band was created</h1>

<dl>
    <dt>Name</dt>
    <dd>{{ entity.name }}</dd>
    <dt>Description</dt>
    <dd>{{ entity.description }}</dd>
    <dt>Status</dt>
    <dd>{{ entity.status }}</dd>
    <dt>Assignee</dt>
    <dd>{{ entity.assignee }}</dd>
    <dt>Owner</dt>
    <dd>{{ entity.owner }}</dd>
    <dt>Created at</dt>
    <dd>{{ entity.createdAt|oro_format_datetime }}</dd>
</dl>